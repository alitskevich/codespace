{
  "name": "CRUD",
  "description": "CRUD",
  "version": "0.1",
  "minimum_chrome_version": "10.0",
  "manifest_version": 3,
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["https://script.google.com/*"],
  "background": {
    "service_worker": "chromext_service_worker.js"
  },
  "action": {
    "default_icon": {
      "16": "favicon/favicon-16x16.png"
    }
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
}
